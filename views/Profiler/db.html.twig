{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar%}
    {% set icon %}
    <span>
            <img height="28" alt="Pomm" src="data:image/png;base64," />
            <span class="sf-toolbar-status{% if 50 < collector.querycount %} sf-toolbar-status-yellow{% endif %}">{{ collector.querycount }}</span>
        {% if collector.querycount > 0 %}
            <span class="sf-toolbar-info-piece-additional-detail">in {{ '%0.2f'|format(collector.time) }} ms</span>
        {% endif %}
        </span>
    {% endset %}

    {% set text %}
    <div class="sf-toolbar-info-piece">
        <b>DB Queries</b>
        <span>{{ collector.querycount }}</span>
    </div>
    <div class="sf-toolbar-info-piece">
        <b>Query time</b>
        <span>{{ '%0.2f'|format(collector.time) }} ms</span>
    </div>
    {% endset %}

    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon"><img src="data:image/png;base64," alt="" /></span>
        <strong>Pomm</strong>
        <span class="count">
            <span>{{ collector.querycount }}</span>
            <span>{{ '%0.0f'|format(collector.time) }} ms</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    <h2>Queries</h2>

    {% if collector.queries is empty %}
        <p>
            <em>No queries.</em>
        </p>
    {% else %}
        <table>
            {% for i, query in collector.queries %}

                <tr data-extra-info="{{ '%0.2f'|format(query.time_ms) }}" data-target-id="{{ i }}">
                    <th id="queryNo-{{ i }}" colspan="2">
                        <strong>Parameters</strong>: {{ query.parameters|yaml_dump }} |
                        <strong>Time</strong>: {{ '%0.2f'|format(query.time_ms) }} ms |
                        <strong>Results</strong>: {{ query.result_count }} |
                        <strong>Entity</strong>: {% if query.flexible_entity is defined %}{{ query.flexible_entity }}{% else %} - {% endif %}
                        <span onClick="openElt(this);" class="pointer" data-type="table-row" data-target="queryDetail-{{ i }}" >[Open +]</span>
                    </th>
                </tr>
                <tr class="querydetail" id="queryDetail-{{ i }}">
                    <td style="min-width: 50%; vertical-align: top;">
                        <code id="code-{{ i }}" style="font-size: 14px;">
                            {{ query.sql|sql_format|raw }}
                        </code>
                        <span onClick="openElt(this);" class="pointer" data-type="table-cell" data-target="explainDetail-{{ i }}">[Explain +]</span>
                    </td>
                    <td style="display:none; width: 50%; vertical-align: top;padding:0px;" id="explainDetail-{{ i }}">
                        {{ collector.renderExplain(app, i) | raw }}
                    </td>
                </tr>
            {% endfor %}
        </table>
    {% endif %}


    <script type="text/javascript">//<![CDATA[
        function openElt(elt) {
            "use strict";
            var $elt = document.getElementById(elt.getAttribute('data-target'));
            $elt.style.display = elt.getAttribute('data-type');
        }
        //]]></script>

    <style>
        .querydetail{
            display: none;
        }
        .pointer{
            cursor: pointer;
        }
    </style>
{% endblock %}
